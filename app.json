{
    "expo": {
        "name": "hutech-cateen-app",
        "slug": "hutech-cateen-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.jpg",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "backgroundColor": "#fefefe",
        "splash": {
            "image": "./assets/images/icon.jpg",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true,
            "newArchEnabled": true,
            "infoPlist": {
                "NSLocationWhenInUseUsageDescription": "Ứng dụng cần quyền truy cập vị trí của bạn để hiển thị dịch vụ tốt hơn."
            },
            "bundleIdentifier": "com.samsamqn258.hutechcateenapp"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/icon.jpg",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "newArchEnabled": false
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/icon.jpg"
        },
        "plugins": ["expo-router", "expo-font"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "229bc1e7-51e1-4c7f-a0b5-63d1351aef32"
            }
        },
        "owner": "samsamqn258"
    }
}
